{
  "openapi": "3.0.2",
  "info": {
    "title": "Nomination Management",
    "description": "Use this service to create, update, or delete successor and talent pool nominations. You can find your organization's API server at [List of API Servers in SAP SuccessFactors](https://help.sap.com/docs/SAP_SUCCESSFACTORS_PLATFORM/9f5f060351034d98990213d077dab38a/af2b8d5437494b12be88fe374eba75b6.html).",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://{api-server}/odatav4/talent/succession/NominationService.svc/v1",
      "description": "Production Url",
      "variables": {
        "api-server": {
          "default": "apisalesdemo8.successfactors.com"
        }
      }
    }
  ],
  "tags": [],
  "paths": {
    "/refreshMetadata": {
      "post": {
        "summary": "Refresh metadata",
        "description": "Refresh the metadata in the Nomination service.",
        "tags": [
          "Refresh Metadata"
        ],
        "responses": {
          "204": {
            "description": "Success"
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json" : {
                "schema" : {
                  "$ref": "#/components/schemas/error"

                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/json" : {
                "schema" : {
                  "$ref": "#/components/schemas/error"

                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json" : {
                "schema" : {
                  "$ref": "#/components/schemas/error"

                }
              }
            }
          }
        }
      }
    },
    "/upsertNomination": {
      "post": {
        "parameters": [
          {
            "in": "query",
            "name": "isPoolNomination",
            "description": "Whether the nomination target is a talent pool. To manage a talent pool nomination instead of a successor nomination, add isPoolNomination=true to the request URI.",
            "schema": {
              "type": "boolean"
            },
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "The nomination was created or updated.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NominationService.svc.nominationActionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request. Possible reasons:\n\nThe nominationTarget or userId field is empty.\n\nThe value of the readiness or rank field is invalid.\n\nThe nomination record does't exist.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "403": {
            "description": "Forbidden. You don't have the permission to access Nomination.svc.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Not Found. The value of the nominationTarget or userId field is invalid.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json" : {
                "schema" : {
                  "$ref": "#/components/schemas/error"

                }
              }
            }
          },
          "501": {
            "description": "Not Implemented. The nomination method is not MDF position-based.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          }
        },
        "summary": "Create or update a nomination for a position or talent pool",
        "description": "Nominate a successor to a position or update the successor information of a position. Or, add or update a talent pool nomination.",
        "x-action": false,
        "x-function": false,
        "tags": [
          "Upsert Nomination"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "root node",
                "properties": {
                  "nominationTarget": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "A position code or a talent pool code. To manage successor nominations, you must use the MDF position-based nomination method.",
                    "type": "string"
                  },
                  "userId": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "User ID of a successor or a talent pool nominee.",
                    "type": "string"
                  },
                  "readiness": {
                    "nullable": false,
                    "x-insertable": true,
                    "example": 2,
                    "description": "Readiness of a successor or a talent pool nominee.",
                    "anyOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "number"
                      }
                    ],
                    "format": "int64"
                  },
                  "rank": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "Rank of a successor or a talent pool nominee.",
                    "type": "integer",
                    "format": "int32"
                  },
                  "note": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "Note for the nomination.",
                    "type": "string"
                  },
                  "emergencyCover": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "Whether a person is an emergency cover for a position or a talent pool.",
                    "type": "boolean"
                  }
                },
                "x-isEntity": true,
                "type": "object"
              }
            }
          }
        }
      }
    },
    "/deleteNomination": {
      "post": {
        "parameters": [
          {
            "in": "query",
            "name": "isPoolNomination",
            "description": "Whether the nomination target is a talent pool. To manage a talent pool nomination instead of a successor nomination, add isPoolNomination=true to the request URI.",
            "schema": {
              "type": "boolean"
            },
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "The nomination was deleted.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NominationService.svc.nominationActionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request. Possible reasons:\n\nThe nominationTarget or userId field is empty.\n\nThe value of the readiness or rank field is invalid.\n\nThe nomination record does't exist.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "403": {
            "description": "Forbidden. You don't have the permission to access Nomination.svc.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Not Found. The value of the nominationTarget or userId field is invalid.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          },
          "501": {
            "description": "Not Implemented. The nomination method is not MDF position-based.",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/error"
                    }
                  ]
                }
              }
            }
          }
        },
        "summary": "Delete a nomination for a position or talent pool",
        "description": "Delete the nomination information of a position or a talent pool.",
        "x-action": false,
        "x-function": false,
        "tags": [
          "Delete Nomination"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "root node",
                "properties": {
                  "nominationTarget": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "A position code or a talent pool code. To manage successor nominations, you must use the MDF position-based nomination method.",
                    "type": "string"
                  },
                  "userId": {
                    "nullable": false,
                    "x-insertable": true,
                    "description": "User ID of a successor or a talent pool nominee.",
                    "type": "string"
                  }
                },
                "x-isEntity": true,
                "type": "object"
              }
            }
          }
        }
      }
    },
    "/$batch": {
      "post": {
        "summary": "Send a group of requests",
        "description": "Group multiple requests into a single request payload. For more information, see [Batch Requests](http://docs.oasis-open.org/odata/odata/v4.01/odata-v4.01-part1-protocol.html#sec_BatchRequests).\n\n*Please note that \"Try Out\" is not supported for this request.*",
        "tags": [
          "Send Batch Requests"
        ],
        "requestBody": {
          "required": true,
          "description": "Batch request",
          "content": {
            "multipart/mixed;boundary=request-separator": {
              "schema": {
                "type": "string"
              },
              "example": "--request-separator\nContent-Type: application/http\nContent-Transfer-Encoding: binary\n\nGET undefined HTTP/1.1\nAccept: application/json\n\n\n--request-separator--"
            }
          }
        },
        "responses": {
          "200": {
            "description": "Batch response",
            "content": {
              "multipart/mixed": {
                "schema": {
                  "type": "string"
                },
                "example": "--response-separator\nContent-Type: application/http\n\nHTTP/1.1 200 OK\nContent-Type: application/json\n\n{...}\n--response-separator--"
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json" : {
                "schema" : {
                  "$ref": "#/components/schemas/error"

                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/json" : {
                "schema" : {
                  "$ref": "#/components/schemas/error"

                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NominationService.svc.nominationActionResponse": {
        "type": "object",
        "properties": {
          "action": {
            "type": "string",
            "nullable": true,
            "x-insertable": false,
            "x-filterable": true
          },
          "value": {
            "type": "boolean",
            "nullable": true,
            "x-insertable": false,
            "x-filterable": true
          }
        },
        "x-insertable": false,
        "x-updatable": false,
        "x-deletable": false,
        "x-isEntity": false,
        "title": "nominationActionResponse"
      },
      "count": {
        "anyOf": [
          {
            "type": "number"
          },
          {
            "type": "string"
          }
        ],
        "description": "The number of entities in the collection. Available when using the [$count](http://docs.oasis-open.org/odata/odata/v4.01/odata-v4.01-part1-protocol.html#sec_SystemQueryOptioncount) query option."
      },
      "error": {
        "type": "object",
        "required": [
          "error"
        ],
        "properties": {
          "error": {
            "type": "object",
            "required": [
              "code",
              "message"
            ],
            "properties": {
              "code": {
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "target": {
                "type": "string"
              },
              "details": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "code",
                    "message"
                  ],
                  "properties": {
                    "code": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    },
                    "target": {
                      "type": "string"
                    }
                  }
                }
              },
              "innererror": {
                "type": "object",
                "description": "The structure of this object is service-specific"
              }
            }
          }
        }
      }
    },
    "parameters": {
      "top": {
        "name": "$top",
        "in": "query",
        "description": "Show only the first n items, see [Paging - Top](http://docs.oasis-open.org/odata/odata/v4.01/odata-v4.01-part1-protocol.html#sec_SystemQueryOptiontop)",
        "schema": {
          "type": "integer",
          "minimum": 0
