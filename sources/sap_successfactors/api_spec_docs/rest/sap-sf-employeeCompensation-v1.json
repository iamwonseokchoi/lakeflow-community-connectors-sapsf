{
  "openapi": "3.0.1",
  "info": {
    "title": "Compensation Management",
    "version": "1.0.0",
    "description" : "Use this API to access data from standard and custom fields on the three tabs of salary, bonus, and stock on the compensation worksheets as well as bonus related data on the variable pay worksheets. This includes user data, any salary related data, as well as data related to performance rating, and force comments."
  },
  "servers": [
    {
      "url": "https://{api-server}/rest/rewards/compensation/v1",
      "description": "REST APIs for the SuccessFactors Data Centers",
      "variables": {
        "api-server": {
          "description": "SuccessFactors Data Center host",
          "default": "apisalesdemo8.successfactors.com"
        }
      }
    }
  ],
  "x-sap-shortText": "Access data from compensation planning",
  "paths": {
    "/employeeCompensations": {
      "get": {
        "description": "Retrieves a list of compensation records from salary, stock, bonus, and variable pay. It checks for permission to read compensation data.",
        "tags": [
          "Employee Compensation"
        ],
        "operationId": "queryEmployeeCompensationEntries",
        "parameters": [
          {
            "name": "templateId",
            "in": "query",
            "required": true,
            "description": "Template ID",
            "schema": {
              "type": "integer",
              "format": "int64",
              "example": 100
            }
          },
          {
            "name": "$skip",
            "in": "query",
            "required": false,
            "description": "The number of events to skip before beginning to return events in the response",
            "schema": {
              "type": "integer",
              "format": "int32",
              "example": 200
            }
          },
          {
            "name": "$top",
            "in": "query",
            "required": false,
            "description": "The maximum number of events to return in the response",
            "schema": {
              "type": "integer",
              "format": "int32",
              "example": 25
            }
          },
          {
            "name": "$filter",
            "in": "query",
            "required": false,
            "description": "The filter operation (eq and in) for compPlanId, formDataId, location, department, and division",
            "schema": {
              "type": "string",
              "example": "compPlanId eq 100 and department in ('Enterprises')"
            }
          },
          {
            "name": "$expand",
            "in": "query",
            "required": false,
            "description": "The expand operation for salary, bonus, stock, varpay, commentList, and ratingInfos",
            "schema": {
              "type": "string",
              "example": "$expand=salary($select=curSalary)"
            }
          },
          {
            "name": "$select",
            "in": "query",
            "required": false,
            "description": "The selection operation for standard and custom fields",
            "schema": {
              "type": "string",
              "example": "$select=curSalary,customFields/customField1"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "It indicates that the request is successful.",
            "headers": {
              "X-Correlation-ID": {
                "$ref": "#/components/headers/X-Correlation-ID"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PageEmployeeCompEntryDTO"
                }
              },
              "application/schema-instance+json": {
                "schema": {
                  "$ref": "#/components/schemas/PageEmployeeCompEntryDTO"
                }
              }
            }
          },
          "400": {
            "description": "It indicates that the server cannot process the request due to something that is perceived to be a client error. For example, templateId is not valid.",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Bad Request",
                  "type": "string"
                }
              }
            }
          },
          "403": {
            "description": "It indicates that the server cannot process the request as the user does not have the required permission to access the resource.",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Forbidden",
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "It indicates that the server has not found anything matching the request URI or the requested resource doesn't exist.",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Not Found",
                  "type": "string"
                }
              }
            }
          },
          "503": {
            "description": "It indicates that the server is not responding.",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Server Unavailable",
                  "type": "string"
                }
              }
            }
          }
        },
        "summary": "Retrieves the compensation records on worksheets for a template ID based on your criteria."
      }
    }
  },
