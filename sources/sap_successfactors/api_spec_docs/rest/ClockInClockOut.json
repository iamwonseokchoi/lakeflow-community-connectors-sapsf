{
  "openapi": "3.0.1",
  "info": {
    "title": "Clock In Clock Out",
    "version": "1.0.0",
    "description": "You can use these APIs to track employee's time punches while using external time tracking services. Please enter the API Key in the format: Bearer<space><token>.\n\n You can find your company's API server in [List of API Servers in SAP SuccessFactors.](https://help.sap.com/viewer/d599f15995d348a1b45ba5603e2aba9b/LATEST/en-US/af2b8d5437494b12be88fe374eba75b6.html)"
  },
  "servers": [
    {
      "url": "https://{api-server}/rest/timemanagement/timeeventprocessing/v1",
      "description": "REST APIs for the SuccessFactors Data Centers",
      "variables": {
        "api-server": {
          "description": "SuccessFactors Data Center host",
          "default": "apisalesdemo2.successfactors.eu"
        }
      }
    },
    {
      "url": "https://sandbox.api.sap.com/sapsuccessfactorstimetracking",
      "description": "Sandbox Server"
    }
  ],
  "x-sap-shortText": "APIs for SAP SuccessFactors Clock In Clock Out",
  "externalDocs": {
    "description": "Clock In Clock Out API on SAP Help Portal",
    "url": "https://help.sap.com/docs/SAP_SUCCESSFACTORS_TIME_TRACKING/a2a03e747cb44e4497b392579c89d439/21eb213a3c4b442fa875821502b20739.html"
  },
  "tags": [
    {
      "name": "TimeEvents"
    }
  ],
  "paths": {
    "/TimeEvents": {
      "post": {
        "tags": [
          "TimeEvents"
        ],
        "summary": "Create time events for employee's time tracking.",
        "description": "The API is for Professional Services, Customers of Clock In Clock Out feature, and Partner Consultants to integrate SAP SuccessFactors Time Tracking with external time tracking services.",
        "operationId": "createTimeEvents",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/TimeEvents"
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "It indicates that the request is successful and the valid time events given in the request are successfully created.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/CreateTimeEventTerminalResponse"
                }
              }
            }
          },
          "400": {
            "description": "It indicates that the server cannot process the request due to something that is perceived to be a client error for example any validation error in the request.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/TimeEventProcessingApiErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "It indicates that the server encountered an unexpected condition that prevented it from fulfilling the request.",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "INTERNAL_SERVER_ERROR"
                    },
                    "message": {
                      "type": "string",
                      "example": "An error occurred while processing the request. Please contact the system administrator."
                    },
                    "errors": {
                      "type": "array",
                      "items": {
                        "type": "string",
                        "example": "Error occurred while processing Time Event."
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "components": {
    "schemas": {
      "TimeEvents": {
        "required": [
          "assignmentId",
          "id",
          "timestamp",
          "typeCode"
        ],
        "type": "object",
        "properties": {
          "id": {
            "maxLength": 36,
            "minLength": 1,
            "pattern": "^([a-zA-Z0-9]+|[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})$",
            "type": "string",
            "description": "Unique ID of the item in the API call, used to map the response later to request items.",
            "example": "1"
          },
          "assignmentId": {
            "maxLength": 100,
            "minLength": 1,
            "pattern": "^[a-zA-Z0-9]+$",
            "type": "string",
            "description": "Assignment ID of the employee, whose time event is recorded.",
            "example": "cgrant"
          },
          "typeCode": {
            "maxLength": 20,
            "minLength": 1,
            "pattern": "[\\w-]+",
            "type": "string",
            "description": "The code of the time event type that was used for the time event.",
            "example": "P10"
          },
          "timestamp": {
            "type": "string",
            "description": "The date and time of the time event in ISO format yyyy-MM-dd'T'HH:mm:ssZ for example, 2020-12-31T10:00:12+05:30 or 2020-12-31T10:00:12+0530.",
            "example": "2020-07-17T12:10:21+02:00"
          },
          "terminalId": {
            "maxLength": 30,
            "minLength": 1,
            "pattern": "^\\w+([\\s-]\\w+)*$",
            "type": "string",
            "description": "Identifier of the terminal at which the time event was recorded.",
            "example": "Building 1 Terminal 5"
          },
          "timeTypeCode": {
            "maxLength": 128,
            "minLength": 1,
            "pattern": "^\\s*[\\da-zA-Z_*\\-][\\da-zA-Z\\s_*\\-]*$",
            "type": "string",
            "description": "The external code of a time type that is available in Time Tracking. This field is only respected if filled in a time event, which has a time event type of type START assigned.",
            "example": "WORKING_TIME"
          }
        }
      },
      "TimeEventProcessingApiErrorResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "BAD_REQUEST",
              "100 CONTINUE",
              "101 SWITCHING_PROTOCOLS",
              "102 PROCESSING",
              "103 CHECKPOINT",
              "200 OK",
              "201 CREATED",
              "202 ACCEPTED",
              "203 NON_AUTHORITATIVE_INFORMATION",
              "204 NO_CONTENT",
              "205 RESET_CONTENT",
              "206 PARTIAL_CONTENT",
              "207 MULTI_STATUS",
              "208 ALREADY_REPORTED",
              "226 IM_USED",
              "300 MULTIPLE_CHOICES",
              "301 MOVED_PERMANENTLY",
              "302 FOUND",
              "302 MOVED_TEMPORARILY",
              "303 SEE_OTHER",
              "304 NOT_MODIFIED",
              "305 USE_PROXY",
              "307 TEMPORARY_REDIRECT",
              "308 PERMANENT_REDIRECT",
              "400 BAD_REQUEST",
              "401 UNAUTHORIZED",
              "402 PAYMENT_REQUIRED",
              "403 FORBIDDEN",
              "404 NOT_FOUND",
              "405 METHOD_NOT_ALLOWED",
              "406 NOT_ACCEPTABLE",
              "407 PROXY_AUTHENTICATION_REQUIRED",
              "408 REQUEST_TIMEOUT",
              "409 CONFLICT",
              "410 GONE",
              "411 LENGTH_REQUIRED",
              "412 PRECONDITION_FAILED",
              "413 PAYLOAD_TOO_LARGE",
              "413 REQUEST_ENTITY_TOO_LARGE",
              "414 URI_TOO_LONG",
              "414 REQUEST_URI_TOO_LONG",
              "415 UNSUPPORTED_MEDIA_TYPE",
              "416 REQUESTED_RANGE_NOT_SATISFIABLE",
              "417 EXPECTATION_FAILED",
              "418 I_AM_A_TEAPOT",
              "419 INSUFFICIENT_SPACE_ON_RESOURCE",
              "420 METHOD_FAILURE",
              "421 DESTINATION_LOCKED",
              "422 UNPROCESSABLE_ENTITY",
              "423 LOCKED",
              "424 FAILED_DEPENDENCY",
              "425 TOO_EARLY",
              "426 UPGRADE_REQUIRED",
              "428 PRECONDITION_REQUIRED",
              "429 TOO_MANY_REQUESTS",
              "431 REQUEST_HEADER_FIELDS_TOO_LARGE",
              "451 UNAVAILABLE_FOR_LEGAL_REASONS",
              "500 INTERNAL_SERVER_ERROR",
              "501 NOT_IMPLEMENTED",
              "502 BAD_GATEWAY",
              "503 SERVICE_UNAVAILABLE",
              "504 GATEWAY_TIMEOUT",
              "505 HTTP_VERSION_NOT_SUPPORTED",
              "506 VARIANT_ALSO_NEGOTIATES",
              "507 INSUFFICIENT_STORAGE",
              "508 LOOP_DETECTED",
              "509 BANDWIDTH_LIMIT_EXCEEDED",
              "510 NOT_EXTENDED",
              "511 NETWORK_AUTHENTICATION_REQUIRED"
            ]
          },
          "message": {
            "type": "string",
            "example": "Request Validation Error"
          },
          "errors": {
            "type": "array",
            "items": {
              "type": "string",
              "example": "The id must not be blank."
            }
          }
        }
      },
      "CreateTimeEventTerminalResponse": {
        "type": "object",
        "properties": {
          "succeededTimeEvents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SucceededTimeEvent"
            }
          },
          "failedTimeEvents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FailedTimeEvent"
            }
          }
        }
      },
      "FailedTimeEvent": {
        "type": "object",
        "properties": {
          "errorText": {
            "type": "string",
            "example" : "A time event for this employee and timestamp already exists."
          },
          "id": {
            "type": "string",
            "example": "2"
          }
        }
      },
      "SucceededTimeEvent": {
        "type": "object",
        "properties": {
          "UUID": {
            "type": "string",
            "example": "4f5a1b86108a4a2195048716593b864e",
            "description": "The internal unique identifier of the successfully created Time Event"
          },
          "id": {
            "type": "string",
            "example": "1",
            "description": "The unique ID of the item mapped from the request"
          }
        }
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  }
}