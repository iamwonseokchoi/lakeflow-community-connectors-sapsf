            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "validTo",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "symbolicAccount",
